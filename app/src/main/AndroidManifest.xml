<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.rowing.coepbc.repechage"
    xmlns:android="http://schemas.android.com/apk/res/android">

  <application
      android:allowBackup="true"
      android:icon="@drawable/repechage_logo"
      android:label="@string/app_name"
      android:theme="@style/AppTheme">
    <activity
        android:name=".MainActivity"
        android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity
        android:name=".ParticipantEntryActivity"
        android:label="@string/participants_activity_label">
    </activity>

    <activity
        android:name=".RoundDetailsActivity"
        android:label="@string/round_details">
    </activity>

    <activity
        android:name=".FinalResultActivity"
        android:label="@string/result">
    </activity>

  </application>

</manifest>
